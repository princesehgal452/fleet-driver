  "scripts": {
    "start-bigroad": "cross-env appMode=BIGROAD webpack-dev-server --open --hot --config ./webpack/webpack.dev.config.js",
    "start-geotab": "cross-env appMode=GEOTAB webpack-dev-server --open --hot --config ./webpack/webpack.dev.config.js",
    "start-aisin": "cross-env appMode=AISIN webpack-dev-server --open --hot --config ./webpack/webpack.dev.config.js",
    "start-v3": "cross-env appMode=V3 webpack-dev-server --open --hot --config ./webpack/webpack.dev.config.js",
    "start:prod": "cross-env appMode=BIGROAD NODE_ENV=production webpack-dev-server --open --hot --config ./webpack/webpack.prod.config.js",
    "build-bigroad": "cross-env appMode=BIGROAD NODE_ENV=production webpack --progress --config ./webpack/webpack.prod.config.js",
    "build-v3": "cross-env appMode=V3 NODE_ENV=production webpack --progress --config ./webpack/webpack.prod.config.js",
    "build-geotab": "cross-env appMode=GEOTAB NODE_ENV=production webpack --progress --config ./webpack/geotab/webpack.prod.config.js",
    "build-aisin": "cross-env appMode=AISIN NODE_ENV=production webpack --progress --config ./webpack/webpack.prod.config.js",
    "stats": "cross-env appMode=BIGROAD NODE_ENV=production webpack --progress --config ./webpack/webpack.stats.config.js --profile --json > ./webpack/stats/stats.json",
    "deploy-staging": "npm run build-bigroad && aws s3 sync build/ s3://staging-driver.fleetops.ai/",
    "deploy-staging-v3": "npm run build-v3 && aws s3 sync build/ s3://v3-staging.fleetops.ai/",
    "deploy-staging-geotab": "npm run build-geotab && aws s3 sync build/ s3://geotab-staging.fleetops.ai/",
    "deploy-staging-aisin": "npm run build-aisin && aws s3 sync build/ s3://staging2.bigroadfreight.com/",
    "deploy-prod": "npm run build-bigroad && aws s3 sync build/ s3://driver.fleetops.ai/",
    "deploy-prod-geotab": "npm run build-geotab && aws s3 sync build/ s3://geotab.fleetops.ai/",
    "test": "jest --forceExit --detectOpenHandles"
  },
